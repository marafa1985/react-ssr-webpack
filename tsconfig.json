{
    "compilerOptions": {
        "outDir": "./public/",
        "noImplicitAny": true,
        "module": "es6",
        "target": "es5",
        // "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": false,
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "react"
    }
}